<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.dg.s1.bankbook.BankbookDAO">
  
  	<delete id="setDelete" parameterType="java.lang.Long">
  		DELETE BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
  	</delete>
  	
  	<insert id="setInsert" parameterType="com.dg.s1.bankbook.BankbookDTO">
  		INSERT INTO BANKBOOK (bookNumber, bookName, bookRate, bookSale)
  		VALUES (bankbook_seq.nextval, #{bookName}, #{bookRate}, #{bookSale})
  	</insert>
  	
  	<select id="getList" resultType="com.dg.s1.bankbook.BankbookDTO">
  		SELECT * FROM BANKBOOK
  	</select>
  	
  	<select id="getSelect" parameterType="com.dg.s1.bankbook.BankbookDTO" resultType="com.dg.s1.bankbook.BankbookDTO">
  		SELECT * FROM BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
  	</select>
	
	
  </mapper>